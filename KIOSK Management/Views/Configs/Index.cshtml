@model IEnumerable<KIOSK_Management.Models.Config>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>variable_name</th>
        <th>value</th>
        <th></th>
    </tr>

@foreach (var item in Model) {
<tr>
    <td>
        @item.variable_name
    </td>
    <td>
        @item.value
    </td>
    <td>
        @Html.ActionLink("Edit", "Edit", new { id = item.variable_name })
    </td>
</tr>
}

</table>
